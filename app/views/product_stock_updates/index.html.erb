<h1>Listing product_stock_updates</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Quantity</th>
      <th>Serial numbers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_stock_updates.each do |product_stock_update| %>
      <tr>
        <td><%= product_stock_update.product_id %></td>
        <td><%= product_stock_update.quantity %></td>
        <td><%= product_stock_update.serial_numbers %></td>
        <td><%= link_to 'Show', product_stock_update %></td>
        <td><%= link_to 'Edit', edit_product_stock_update_path(product_stock_update) %></td>
        <td><%= link_to 'Destroy', product_stock_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product stock update', new_product_stock_update_path %>
