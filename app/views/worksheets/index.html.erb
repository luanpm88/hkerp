<ul class="breadcrumb">
    <li>
        <p>YOU ARE HERE</p>
    </li>
    <li>
        <%= link_to 'Worksheet', worksheets_url, :class => "active" %>
    </li>
</ul>
<div class="page-title">
    <i class="icon-user"></i>
    <h3>Worksheet - <span class="semi-bold"> Management</span></h3>
</div>

<div class="grid simple ">
    <div class="grid-body no-border filters" method="get">
        <%= render "filter" %>
    </div>
</div>


<div class="row">
    <div class="col-md-12">
        <%= link_to 'New Worksheet', new_worksheet_path(tab_page: 1), title: "New Worksheet", psrc: worksheets_path(tab_page: 1), :class => 'btn btn-primary btn-cons tab_page' %>
            
        <div class="grid simple ">
            <div class="grid-title no-border">
                <h4>Listing  <span class="semi-bold">Worksheets</span></h4>
                <div class="tools">

                    <a href="javascript:;" class="collapse"></a>
                    <a href="#grid-config" data-toggle="modal" class="config"></a>
                    <a href="javascript:;" class="reload"></a>
                    <a href="javascript:;" class="remove"></a>

                </div>
            </div>
            <div class="grid-body no-border">

                <table class="table table-striped no-more-tables datatable-worksheets no-filter">
                    <thead>
                        <tr>
                            <th class="text-left">Staff Name</th>
                            <th class="text-center">Status</th>
                            <th class="text-center">Created At</th>
                            <th class="text-center">Creator</th>
                            <th class="text-right">Actions</th>
                        </tr>
                    </thead>
                    <tbody>


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>